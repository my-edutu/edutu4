# Edutu Opportunities Aggregator Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development

# Google Custom Search API Configuration (REQUIRED)
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_CX=your_custom_search_engine_id_here

# Cache Configuration
CACHE_TIMEOUT_MS=3600000

# API Configuration
DEFAULT_PAGE_SIZE=10
MAX_PAGE_SIZE=50

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://your-domain.com

# Logging Configuration
LOG_LEVEL=info